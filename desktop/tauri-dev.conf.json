{
    "build": {
        "beforeDevCommand": "npm run serve:dev",
        "beforeBuildCommand": "npm run build:dev",
        "devPath": "http://localhost:1420",
        "distDir": "../dist",
        "withGlobalTauri": false

    },
    "package": {
        "productName": "PolyScrabble - dev",
        "version": "0.0.1"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "window": {
                "all": true
            },
            "dialog": {
                "all": true
            },
            "http": {
                "scope": ["http://**", "https://**", "ws://**"],
                "all": true,
                "request": true
            },
            "shell": {
                "all": false,
                "open": true
            },
            "fs": {
                "scope": ["$RESOURCE/*"]
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
            "identifier": "com.polyscrabble.dev",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "ec2-35-183-107-112.ca-central-1.compute.amazonaws.com localhost",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [
                "../certs/server.pem",
                "../certs/server.key"
            ],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": "default-src blob: data: filesystem: ws: wss: http: http://localhost https://localhost https: tauri: tauri://localhost 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'; script-src 'self' https://www.google.com https://www.gstatic.com 'unsafe-eval'"
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 800,
                "resizable": true,
                "title": "PolyScrabble - dev",
                "width": 1000,
                "focus": false,
                "minWidth": 1000,
                "minHeight": 800,
                "label": "main"
            }
        ]
    }
}
